%tr
  %th{ data: { :'variants-matrix-variant-enabled-cell' => true } }
    = t('para.stall.variants_matrix.enabled')

  - properties.each do |variants_property_config|
    %th{ data: { :'variants-matrix-variant-property-cell' => true } }
      = variants_property_config.property.name

  %th= t('para.stall.variants_matrix.price')

  - if Stall.config.manage_inventory
    %th= t('para.stall.variants_matrix.stock')

  %th
